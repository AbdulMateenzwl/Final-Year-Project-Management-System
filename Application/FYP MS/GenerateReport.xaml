<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FYP_MS.HelperClasses"
             
            x:Class="FYP_MS.HelperClasses.GenerateReport"
             

             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel Background="AliceBlue" >
        <Grid DockPanel.Dock="Top" UseLayoutRounding="False" Cursor="Hand">
            <Grid.RowDefinitions>
                <RowDefinition Height="50"/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <WrapPanel>
                <Label Margin="10 10 0 10" Content="Search"/>
                <Button Content="Create Report" Margin="5" Click="CreateReport_Click" Width="80" Style="{StaticResource Btntype1}"/>
                <Button Content="Update Student" Margin="5" Click="Button_Click" Width="100" Style="{StaticResource Btntype1}"/>
            </WrapPanel>
            <!--<WebBrowser Grid.Row="1" x:Name="pdfviewer"></WebBrowser>-->
        </Grid>
    </DockPanel>
</UserControl>
